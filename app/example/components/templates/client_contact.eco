<h3>Client: <%= "#{@first_name} #{@last_name}" %> - Contact Info</h3>
<a href="#client/<%= @id %>">Details</a> | Contact Info
<hr>
<% for phone in @items: %>
  Phone: <%= phone.number %><br>
  Extension: <%= phone.ext || '-' %><br>
  Type: <%= phone.type || '-' %>
  <hr>
<% end %>
<a ui-sref="app.client.overview">test ui-sref 1</a> |
<a ui-sref="app.client.overview({id: 2})">test ui-sref 2</a>
